public class PassArgumentIntoMethod 
{
    public static void main(String[] args) 
    {
        int x= 10;
        
        System.out.println("I am passing values to the displayValue.");
        displayValue(5);    // These are message calls
        displayValue(x);
        displayValue(x*4);
        displayValue(Integer.parseInt("700"));
        System.out.println( "Now I am back in main. ");
        
    }
  
   
    
    /**
     * The displayValue method displays the value of its integer parameter.
     */
     
    
     /** 
      * 
      * @param num */
      
                public static void displayValue(int num)
                 { 
                     System.out.println( "The value is "+ num);
                  }
             
        
           
        }
             
